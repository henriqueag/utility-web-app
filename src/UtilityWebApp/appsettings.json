{
    "urls": "http://*:5292",
    
    "Serilog": {
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Microsoft": "Error",
                "Microsoft.Hosting": "Information",
                "System": "Error"
            }
        },
        "WriteTo": [
            "Console"
        ],
        "Enrich": [
            "FromLogContext"
        ]
    },
    "AllowedHosts": "*"
}
